{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "outDir":"lib",
    "sourceMap": true,
    "skipLibCheck":true,
    "lib": [
      "es2021",
      "ES2021.String"
    ]
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
 }
